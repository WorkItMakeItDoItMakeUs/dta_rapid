{
  "name": "dta_rapid",
  "repository": "git@github.com:gtrogers/dta_rapid.git",
  "author": "@WorkItMakeItDoItMakeUs",
  "scripts": {
    "build": "./node_modules/.bin/node-sass src/sass/main.scss assets/css/style.min.css --output-style compressed",
    "watch:sass": "npm run build && onchange 'src/sass/*.scss' -- npm run build",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "devDependencies": {
    "browser-sync": "^2.18.8",
    "node-sass": "^4.5.1",
    "npm-run-all": "^4.0.2",
    "onchange": "^3.2.1"
  },
  "dependencies": {
    "@gov.au/body": "^0.1.0",
    "@gov.au/buttons": "^0.3.0",
    "@gov.au/callout": "^0.1.0",
    "@gov.au/control-input": "^0.1.0",
    "@gov.au/footer": "^0.1.0",
    "@gov.au/grid-12": "^0.1.0",
    "@gov.au/header": "^1.0.0",
    "@gov.au/page-alerts": "^0.1.1",
    "@gov.au/pancake": "^1.1.0",
    "@gov.au/pancake-sass": "^1.1.0",
    "@gov.au/progress-indicator": "^0.1.0",
    "@gov.au/text-inputs": "^0.1.0",
    "jquery": "^3.2.0",
    "parsley": "^0.1.0"
  },
  "pancake": {
    "auto-save": false,
    "plugins": true,
    "ignore": [],
    "css": {
      "minified": true,
      "modules": false,
      "browsers": [
        "last 2 versions",
        "ie 8",
        "ie 9",
        "ie 10"
      ],
      "location": "pancake",
      "name": "pancake.css"
    },
    "sass": {
      "modules": false,
      "location": "src/sass/",
      "name": "uikit.scss"
    }
  }
}
